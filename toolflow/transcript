add wave -position insertpoint vsim:/tb/MyMips/*
add wave -position insertpoint  \
vsim:/tb/MyMips/s_DMemOut
add wave -position insertpoint vsim:/tb/MyMips/WordDecoder0/*
add wave -position insertpoint vsim:/tb/MyMips/ByteDecoder0/*
add wave -position insertpoint  \
vsim:/tb/MyMips/mux4/i_w0 \
vsim:/tb/MyMips/mux4/i_w1 \
vsim:/tb/MyMips/mux4/i_w2 \
vsim:/tb/MyMips/mux4/i_w3 \
vsim:/tb/MyMips/mux4/i_s0 \
vsim:/tb/MyMips/mux4/i_s1 \
vsim:/tb/MyMips/mux4/o_Y
add wave -position insertpoint  \
vsim:/tb/CLK
add wave -position insertpoint vsim:/tb/MyMips/WordDecoder0/*
add wave -position insertpoint vsim1:/tb/MyMips/*
add wave -position insertpoint  \
vsim1:/tb/MyMips/DMem/q
add wave -position insertpoint  \
vsim1:/tb/CLK
add wave -position insertpoint vsim1:/tb/MyMips/WordDecoder0/*
add wave -position insertpoint vsim1:/tb/MyMips/mux4/*
